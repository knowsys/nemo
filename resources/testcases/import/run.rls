@import sourceA[3]: csv{path = "sources/dataA.csv"}.
@import sourceB[3]: dsv{path = "sources/dataB.csv", delimiter = ","}.
@import sourceC[3]: csv{path = "sources/dataC.csv"}.

J1(?X, ?Y, ?Z) :- sourceA(?X, ?Z, ?Y), sourceB(?X, ?Y, ?T) .
J2(?X, ?Y, ?Z) :- sourceA(?Z, ?Y, ?X), sourceC(?X, ?Y, ?T) .
J3(?X, ?Y, ?W) :- sourceA(?T, ?Y, ?X), sourceB(?T, ?Y, ?X), sourceC(?X, ?Y, ?W) .
@declare a(RdfsResource, UnsignedInteger) .
@declare c(UnsignedInteger) .
@declare numeric(UnsignedInteger, Double) .

@source c[1]: load-csv("sources/c.csv") .
@source numeric[2]: load-csv("sources/numeric.csv") .

a(Hello, ?X) :- c(?X) .
b(?Y, ?X) :- a(?X, ?Y) .
a(?Y, ?X) :- b(?X, ?Y) .
r(?Y, ?X) :- numeric(?X, ?Y) . 
